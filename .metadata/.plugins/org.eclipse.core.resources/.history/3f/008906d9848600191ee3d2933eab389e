
public class DoubleList {
	private DoubleNode head;
	
	DoubleList()
	{
		head = null;
	}
	
	void add(double d)
	{
		DoubleNode next = new DoubleNode(d);
		
		if (head == null)
		{
			head = next;
		}
		else
		{
			DoubleNode current = head;
			
			while(current.getNext() != null)
			{
				current = current.getNext();
			}
		}
	}
}
