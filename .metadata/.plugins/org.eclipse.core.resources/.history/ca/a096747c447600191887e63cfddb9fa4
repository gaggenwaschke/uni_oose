
public class SortDoubles {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

	
	public static double[] sortDouble(double[] a) {
		if (a.length <= 1) return a;
		
		int index_min = 0;
		double min = a[0];
		double temp;
		
		for (int i = 1; i < a.length; i++) {
			if (a[i] < min) {
				min = a[i];
				index_min = i;
			}
		}
		
		a[index_min] = a[0];
		a[0] = min;
	}
}
