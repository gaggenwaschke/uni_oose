import java.util.*;


public class TesteFunktionen {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		IFunktion f = new EFunktion(0.25, 1.75);
		Plotter.druckeWertetabelle(f, 0.0, 10.0);
