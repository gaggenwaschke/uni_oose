
public class TNode {
	private TNode left;
	private TNode right;
	private int value;
	
	public TNode(int value)	{
		this.value = value;
	}
	
	public TNode getLeft() {
		return left;
	}
	public TNode getRight() {
		return right;
	}
	public void setLeft(TNode left) {
		this.left = left;
	}
	public void setRight(TNode right) {
		this.right = right;
	}
	
	public String toString() {
		return this.value.toString();
	}
}
